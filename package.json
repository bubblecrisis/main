{
  "name": "@mocks-server/main",
  "version": "1.8.8",
  "description": "Mocks server supporting multiple api behaviors",
  "keywords": [
    "mocks",
    "server",
    "mock",
    "fixtures",
    "behaviors",
    "api",
    "rest",
    "cli",
    "express",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/main",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "mocks-server": "./bin/mocks-server"
  },
  "scripts": {
    "lint": "eslint *.js lib test",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js --runInBand",
    "test:unit": "npm run test"
  },
  "dependencies": {
    "@mocks-server/core": "1.5.1",
    "@mocks-server/plugin-admin-api": "1.4.7",
    "@mocks-server/plugin-inquirer-cli": "1.3.6"
  },
  "devDependencies": {
    "eslint": "7.16.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.3.0",
    "fs-extra": "9.0.1",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "lodash": "4.17.20",
    "prettier": "2.2.1",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "sinon": "9.2.2",
    "strip-ansi": "6.0.0",
    "tree-kill-sync": "1.0.0"
  },
  "lint-staged": {
    "lib/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x || 15.x"
  }
}
